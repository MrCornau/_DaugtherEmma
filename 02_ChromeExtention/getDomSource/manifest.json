{
    "name": "Get pages source",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Get pages source from a popup",

    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [

                
                "node_modules/jquery/dist/jquery.js",
                "myScript.js"
            ]
        }
    ]
}